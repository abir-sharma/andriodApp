{
  "expo": {
    "name": "CodingMonk",
    "slug": "coding-monk",
    "description":"Best app for coding",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/app-store.png",
    "splash": {
      "image": "./assets/app-store.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/app-store.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
